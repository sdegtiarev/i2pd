{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.bootstrapcdn.com; object-src 'self'",
   "description": "URL Logger",
   "icons": {
      "128": "icon/i2p_128.png",
      "32": "icon/i2p_32.png",
      "64": "icon/i2p_64.png"
   },
   "manifest_version": 2,
   "name": "URLog",
   "options_page": "options.html",
   "permissions": [ "\u003Call_urls>", "webRequest", "webRequestBlocking" ],
   "version": "1.0"
}
